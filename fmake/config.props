msvc.define=/D{define}
msvc.libDir=/LIBPATH:{libDir}
msvc.includeDir=/I{includeDir}
msvc.libList={libName}
msvc.objList={objList}.obj

msvc.comp=cl /c /TP /EHsc /nologo /O2 /MD /DWIN32 /D_WINDOWS {msvc.define} {msvc.includeDir} /Fo{outObjFile}.obj {srcList}
msvc.lib=lib /OUT:{outFile}.lib {msvc.objList}
msvc.exe=link /NOLOGO {msvc.libDir} /OUT:{outFile}.exe {msvc.libList} {msvc.objList}
msvc.dll=link /NOLOGO /DLL {msvc.libDir} /OUT:{outFile}.dll {msvc.libList} {msvc.objList}

gcc.define=-D{define}
gcc.libDir=-L{libDir}
gcc.includeDir=-I{includeDir}
gcc.libList=-l{libName}
gcc.objList={objList}.o

gcc.comp=g++ -fPIC -fpermissive -O3 -Wall -c {gcc.define} {gcc.includeDir} -o{outObjFile}.o {srcList}
gcc.lib=ar cqs {outLibFile}.a {gcc.objList}
gcc.exe=g++ -o {outFile} -pthread {gcc.objList} {gcc.libDir} {gcc.libList}
gcc.dll=g++ -shared -o{outLibFile}.so {gcc.objList} {gcc.libDir} {gcc.libList}

